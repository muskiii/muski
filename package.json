    "cors": "2.7.1",
    "dotenv": "2.0.0",
    "ejs": "2.4.1",
    "errorhandler": "1.4.3",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "graphql": "^14.2.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "methods": "1.1.2",
    "mongoose": "^5.5.0",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "2.0.2",
    "morgan": "^1.9.1",
